# Set the default behavior for text files
* text=auto

# Denote all files that are truly binary and should not be modified
*.png binary
*.jpg binary
*.gif binary
*.pdf binary
*.doc binary
*.docx binary
*.xls binary
*.xlsx binary
*.zip binary
*.gz binary
*.tar binary
*.tgz binary
*.bz2 binary
*.exe binary
*.dll binary
*.so binary
*.o binary
*.a binary
*.lib binary
*.obj binary
*.jar binary
*.class binary
*.pyc binary
*.ipynb binary

# Denote all files that should have their line endings normalized
*.txt text eol=lf
*.md text eol=lf
*.rst text eol=lf
*.html text eol=lf
*.css text eol=lf
*.js text eol=lf
*.json text eol=lf
*.yml text eol=lf
*.yaml text eol=lf
*.toml text eol=lf
*.ini text eol=lf
*.cfg text eol=lf
*.conf text eol=lf
*.cnf text eol=lf
*.sh text eol=lf
*.bash text eol=lf
*.bat text eol=crlf
*.cmd text eol=crlf